{
  "listeners": {
    "*:8000": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "!/index.php"
      },
      "action": {
        "share": "/app/public/",
        "fallback": {
          "pass": "applications/laravel"
        }
      }
    }
  ],
  "applications": {
    "laravel": {
      "type": "php",
      "root": "/app/public/",
      "script": "index.php",
      "options": {
        "admin": {
          "upload_max_filesize": "100M"
        }
      }
    }
  }
}
