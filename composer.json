{
  "name": "ppy/osu-web",
  "description": "osu!web",
  "type": "project",
  "homepage": "https://new.ppy.sh",
  "license": "AGPL-3.0",
  "support": {
    "email": "support@ppy.sh",
    "issues": "https://github.com/ppy/osu-web/issues"
  },
  "require": {
    "doctrine/dbal": "*",
    "ezyang/htmlpurifier": "^4.7",
    "graham-campbell/github": "^5.0",
    "graham-campbell/markdown": "*",
    "itsgoingd/clockwork": "~1.9",
    "laravel/framework": "5.3.*",
    "laravel/passport": "^1.0",
    "laravelcollective/html": "5.3.*",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/fractal": "^0.14.0",
    "league/statsd": "^1.4.0",
    "lord/laroute": "~2.0",
    "maknz/slack": "*",
    "mariuzzo/laravel-js-localization": "*",
    "predis/predis": "~1.0",
    "sentry/sentry-laravel": "^0.6.0",
    "shift31/laravel-elasticsearch": "~1.0",
    "webuni/commonmark-table-extension": "^0.6.1",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
